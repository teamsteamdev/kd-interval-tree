{
  "name": "kd-interval-tree",
  "version": "0.4.1",
  "description": "Create and search layered interval trees",
  "license": "MIT",
  "author": {
    "name": "Jack Steam",
    "email": "jacksteamdev@gmail.com"
  },
  "main": "dist/kd-interval-tree.cjs.js",
  "module": "dist/kd-interval-tree.esm.js",
  "browser": "dist/kd-interval-tree.umd.js",
  "dependencies": {
    "lodash": "^4.17.16",
    "node-interval-tree": "^1.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "start": "rollup -c -w",
    "test": "jest",
    "test-ci":
      "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-watch": "jest --watch",
    "pretest": "eslint src/** test/**"
  },
  "files": ["dist"],
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix src/**",
      "git add"
    ],
    "*.{json,css,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "printWidth": 65,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
